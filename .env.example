# 환경변수 설정 예시
# 복사하여 .env 파일로 만들어 사용하세요

# Azure 설정
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint
AZURE_SPEECH_API_KEY=your_azure_speech_api_key
AZURE_SPEECH_REGION=your_azure_speech_region

# 데이터베이스 설정
COSMOS_DB_URI=your_cosmos_db_uri
COSMOS_DB_KEY=your_cosmos_db_key
COSMOS_DB_NAME=safedining

# Redis 설정
REDIS_URL=redis://localhost:6379

# Naver Maps API
NAVER_MAPS_CLIENT_ID=your_naver_maps_client_id
NAVER_MAPS_CLIENT_SECRET=your_naver_maps_client_secret

# JWT 설정
SECRET_KEY=your_jwt_secret_key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 환경 설정
ENVIRONMENT=development
DEBUG=true
